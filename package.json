    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "email-validator": "^2.0.4",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "jquery": "^3.7.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mysql2": "^3.6.5",
    "node-sass": "^9.0.0",
    "node-sass-middleware": "^1.1.0",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "sass": "^1.69.5",
    "sendgrid": "^5.2.3",
    "sequelize": "^6.35.1",
    "swagger-ui-express": "^5.0.0",
